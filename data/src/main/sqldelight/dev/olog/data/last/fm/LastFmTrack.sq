CREATE TABLE IF NOT EXISTS last_fm_track (
    id INTEGER NOT NULL PRIMARY KEY,
    title TEXT NOT NULL,
    artist TEXT NOT NULL,
    album TEXT NOT NULL,
    image_url TEXT NOT NULL,
    added TEXT NOT NULL,
    mbid TEXT NOT NULL,
    artist_mbid TEXT NOT NULL,
    album_mbid TEXT NOT NULL
);

selectById:
SELECT *
FROM last_fm_track
WHERE id = ? AND added BETWEEN date('now', '-1 month') AND date('now');

insert:
INSERT OR REPLACE INTO last_fm_track VALUES ?;

delete:
DELETE FROM last_fm_track WHERE id = ?;